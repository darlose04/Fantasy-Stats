<% include ../partials/header %>

<div class="container">
  <div class="row">
    <div class="col-lg-12">
      
      <div id="table">
        <table class="table">
          <thead>
            <tr>
              <th scope="col">Team Comparisons</th>
            </tr>
          </thead>
          <tbody>
            <% totals.forEach(function(total) { %>
            <tr>
              <td>
                <a href="/totals/<%= total._id %>">
                  <%= total.name %></a>
              </td>
            </tr>
            <% }) %>
          </tbody>

          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>

<% include ../partials/footer %>