<% include ../partials/header %>

<div class="container">
  <div class="row"> 
    <div class="col-lg-12">
      <div id="table">
        <table class="table">
          <thead>
          <tr>
            <th scope="col">Team Name</th>
          </tr> 
          </thead>
          <tbody>
            <% teams.forEach(function(team) { %>
              <tr>
                <td>
                  <a href="/teams/<%= team._id %>">
                  <%= team.name %></a>
                </td>
              </tr>
            <% }) %>
          </tbody>
          
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>

<% include ../partials/footer %>